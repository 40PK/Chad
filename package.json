{
  "name": "chad",
  "version": "2.0.0",
  "description": "Telegram channel administration tool",
  "main": "main.js",
  "scripts": {
    "start": "electron ./package",
    "package-darwin": "electron-packager ./package Chad --platform=darwin --arch=all --icon=./src/icons/chad --out=./builds --overwrite",
    "package-linux": "electron-packager ./package Chad --platform=linux --arch=all --icon=./src/icons/chad --out=./builds --overwrite",
    "package-win": "electron-packager ./package Chad --platform=win32 --arch=all --icon=./src/icons/chad --out=./builds --overwrite"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Perkovec/Chad.git"
  },
  "keywords": [
    "Telegram",
    "tool",
    "administration",
    "channel",
    "Electron"
  ],
  "license": "GNU GPLv3",
  "author": "Perkovec",
  "bugs": {
    "url": "https://github.com/Perkovec/Chad/issues"
  },
  "homepage": "https://github.com/Perkovec/Chad#readme",
  "devDependencies": {
    "del": "^2.2.1",
    "electron-prebuilt": "^1.3.1",
    "eslint": "^3.2.0",
    "eslint-plugin-react": "^5.2.2",
    "gulp": "^3.9.1",
    "gulp-autoprefixer": "^3.1.0",
    "gulp-clean-css": "^2.0.12",
    "gulp-ext-replace": "^0.3.0",
    "gulp-ignore": "^2.0.1",
    "gulp-react": "^3.1.0",
    "gulp-sass": "^2.3.2",
    "gulp-uglify": "^1.5.4",
    "gulp-useref": "^3.1.0",
    "gulp-watch": "^4.3.9",
    "run-sequence": "^1.2.2",
    "spawn-sync": "^1.0.15",
    "uglify-js-harmony": "^2.6.2"
  },
  "dependencies": {
    "electron-contextmenu-middleware": "^1.0.2",
    "electron-input-menu": "^2.0.3",
    "material-ui": "^0.15.3",
    "react": "^15.3.0",
    "react-dom": "^15.3.0",
    "react-layout-pane": "^0.1.16",
    "react-tap-event-plugin": "^1.0.0",
    "superagent": "^2.1.0",
    "universal-analytics": "^0.4.2",
    "xss": "^0.2.13"
  }
}
